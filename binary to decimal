class HelloWorld {
    public static void bintodec(int n){
        int mynum=n;
        int dec=0;
       int  pow=0;
        while(n>0){
            int lastdig=n%10;
            dec=dec+(lastdig*(int) Math.pow(2,pow ));
            pow++;
            n=n/10;
        }
        System.out.println("binary of " + mynum + " is " + "= " + n);
    }
    
    public static void main(String[] args) {
        bintodec(101);
    }
}
